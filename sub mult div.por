programa {
  funcao inicio() {
    real num1, num2, subtracao, multiplicacao, divisao

    escreva("digite um numero")
    leia(num1)

    escreva("digite outro numero")
    leia(num2)

    subtracao= (num1 - num2)
    multiplicacao = (num1 * num2)
    divisao= (num1/num2)

    escreva("O resltado da subtração é" + subtracao + ", o resultado da multiplicação é" + multiplicacao + "e o resultado da divisão é" + divisao)
  }
}
